<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>MyDigitalServer</display-name>
  <welcome-file-list>
 
    <welcome-file>/jsp/index.jsp</welcome-file>

  </welcome-file-list>
  
  <servlet>
    <servlet-name>FileUploadAction</servlet-name>
    <servlet-class>com.dengwm.handler.FileUploadAction</servlet-class>
  </servlet>
	
  <servlet-mapping>
    <servlet-name>FileUploadAction</servlet-name>
    <url-pattern>/servlet/fileUpload</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>multiparttestservlet</servlet-name>
    <servlet-class>com.dengwm.handler.MultipartTestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>multiparttestservlet</servlet-name>
    <url-pattern>/jsp/servlet/multiparttestservlet</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>diskfileupload</servlet-name>
    <servlet-class>com.dengwm.handler.DiskFileUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>diskfileupload</servlet-name>
    <url-pattern>/jsp/servlet/diskfileupload</url-pattern>
  </servlet-mapping>
  
</web-app>